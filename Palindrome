//Palindrome 
class Solution {
    public boolean isPalindrome(int x) {
        if(x<0){
            return false;
        }
        int temp = x;
        int s= 0;
        while(temp!=0){
            
            int r= temp%10;
            s =s*10+r;
            temp=temp/10;

        }return s==x;
        
      
        
    }
    
}
